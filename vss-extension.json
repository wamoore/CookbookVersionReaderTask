{
  "manifestVersion": 1,
  "id": "CookbookVersionReader",
  "name": "Chef Cookbook Version Reader",
  "version": "1.0.0",
  "publisher": "wkdmoore",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "A build task that reads the version from the metadata.rb file and makes it available as build variables.",
  "categories": [
    "Build and release"
  ],
  "tags": [
    "Chef",
    "Version",
    "Build Variables"
  ],
  "icons": {
    "default": "images/extension-icon.png"
  },
  "files": [
    {
      "path": "CookbookVersionReader"
    }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "license"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/wamoore/CookbookVersionReaderTask"
    },
    "support": {
      "uri": "https://github.com/wamoore/CookbookVersionReaderTask/Issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/wamoore/CookbookVersionReaderTask"
  },
  "galleryFlags": [
    "Public"
  ],
  "contributions": [
    {
      "id": "cookbook-version-reader-build-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "CookbookVersionReaderTask"
      }
    }
  ]
}
